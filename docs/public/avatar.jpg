<svg width="200" height="200" viewBox="0 0 200 200" xmlns="http://www.w3.org/2000/svg">
  <!-- 背景圆形 -->
  <circle cx="100" cy="100" r="100" fill="url(#avatarGradient)"/>
  
  <!-- 渐变定义 -->
  <defs>
    <linearGradient id="avatarGradient" x1="0%" y1="0%" x2="100%" y2="100%">
      <stop offset="0%" style="stop-color:#667eea;stop-opacity:1" />
      <stop offset="100%" style="stop-color:#764ba2;stop-opacity:1" />
    </linearGradient>
    <linearGradient id="faceGradient" x1="0%" y1="0%" x2="100%" y2="100%">
      <stop offset="0%" style="stop-color:#ffeaa7;stop-opacity:1" />
      <stop offset="100%" style="stop-color:#fab1a0;stop-opacity:1" />
    </linearGradient>
  </defs>
  
  <!-- 头发 -->
  <path d="M50 80 Q100 40 150 80 Q140 60 120 65 Q100 50 80 65 Q60 60 50 80" fill="#2d3436"/>
  
  <!-- 脸部 -->
  <circle cx="100" cy="110" r="45" fill="url(#faceGradient)"/>
  
  <!-- 眼睛 -->
  <circle cx="85" cy="100" r="8" fill="#2d3436"/>
  <circle cx="115" cy="100" r="8" fill="#2d3436"/>
  <circle cx="87" cy="98" r="3" fill="#ffffff"/>
  <circle cx="117" cy="98" r="3" fill="#ffffff"/>
  
  <!-- 鼻子 -->
  <ellipse cx="100" cy="115" rx="3" ry="5" fill="#e17055"/>
  
  <!-- 嘴巴 -->
  <path d="M90 125 Q100 135 110 125" stroke="#2d3436" stroke-width="2" fill="none" stroke-linecap="round"/>
  
  <!-- 眼镜 -->
  <circle cx="85" cy="100" r="15" fill="none" stroke="#2d3436" stroke-width="2"/>
  <circle cx="115" cy="100" r="15" fill="none" stroke="#2d3436" stroke-width="2"/>
  <line x1="100" y1="100" x2="100" y2="100" stroke="#2d3436" stroke-width="2"/>
  
  <!-- 衣服 -->
  <path d="M55 155 Q100 170 145 155 L145 200 L55 200 Z" fill="#0984e3"/>
  
  <!-- 领子 -->
  <path d="M80 155 Q100 165 120 155" stroke="#ffffff" stroke-width="2" fill="none"/>
  
  <!-- 装饰性代码符号 -->
  <text x="160" y="40" font-family="monospace" font-size="12" fill="#00b894" opacity="0.7">&lt;/&gt;</text>
  <text x="30" y="170" font-family="monospace" font-size="10" fill="#00b894" opacity="0.5">{ }</text>
  <text x="170" y="180" font-family="monospace" font-size="8" fill="#00b894" opacity="0.5">JS</text>
</svg>